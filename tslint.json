{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "trailing-comma": [ false ],
      "max-classes-per-file": false,
      "no-console": [
        true,
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace"
      ],
      "no-trailing-whitespace": [true, "ignore-comments"],
      "newline-before-return": true,
      "no-consecutive-blank-lines": true
  },
  "rulesDirectory": []
}